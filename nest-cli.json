      "root": "libs/database-pg",
      "entryFile": "index",
      "sourceRoot": "libs/database-pg/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database-pg/tsconfig.lib.json"
      }
    },
    "grades-api": {
      "type": "application",
      "root": "apps/grades-api",
